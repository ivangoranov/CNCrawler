{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schema.json",
  "title": "Notice",
  "description": "Tender notice item",
  "type": "object",
  "properties":{
    "contract_type": {
      "description": "Type of contract for respective notice",
      "type": "string"
    },
    "estimated_value": {
      "description": "The estimated value of contract for respective notice",
      "type": "number"
    },
    "notice_id": {
      "description": "ID of contract for respective notice",
      "type": "integer"
    },
    "notice_number": {
      "description": "Name for respective notice",
      "type": "string"
    },
    "procedure_state": {
      "description": "Procedure state for respective notice",
      "type": "string"
    },
    "type_of_procurement": {
      "description": "Type of procurement of respective notice",
      "type": "string"
    }
  },
  "required": ["contract_type", "estimated_value", "notice_id", "notice_number", "procedure_state", "type_of_procurement"]
}